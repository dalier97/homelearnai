{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-sqlite3": "*",
        "ext-zip": "*",
        "barryvdh/laravel-dompdf": "^3.1",
        "intervention/image": "^3.11",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.2",
        "laravel/tinker": "^2.10.1",
        "league/commonmark": "^2.7",
        "league/csv": "^9.24",
        "league/html-to-markdown": "^5.1",
        "mews/purifier": "^3.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.3",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpstan/phpstan": "*",
        "phpunit/phpunit": "^11.5.3",
        "psalm/plugin-laravel": "^3.0",
        "tomasvotruba/bladestan": "^0.11.3",
        "vimeo/psalm": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "CACHE_STORE=file php artisan serve"
        ],
        "dev-with-queue": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail\" --names=server,queue,logs --kill-others"
        ],
        "test": [
            "@putenv APP_ENV=testing",
            "@putenv DB_DATABASE=learning_app_test",
            "@php artisan config:clear --ansi",
            "./scripts/safe-test.sh"
        ],
        "test:ci": [
            "@putenv APP_ENV=testing",
            "@putenv DB_DATABASE=learning_app_test",
            "@putenv DB_CONNECTION=pgsql",
            "@putenv DB_HOST=localhost",
            "@putenv DB_PORT=5432",
            "@putenv DB_USERNAME=laravel",
            "@putenv DB_PASSWORD=12345",
            "@putenv CACHE_STORE=array",
            "@putenv SESSION_DRIVER=array",
            "@php artisan config:clear --ansi",
            "@php artisan test --no-coverage"
        ],
        "test:safe": [
            "./scripts/safe-test.sh"
        ],
        "phpstan": [
            "./vendor/bin/phpstan analyse --memory-limit=1G --no-progress"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
